generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model codeblocks {
  id         Int       @id @default(autoincrement())
  base_id    String?
  name       String?
  content    String?
  language   String?
  theme      String?
  created_at DateTime? @db.Timestamptz(6)
  updated_at DateTime? @db.Timestamptz(6)
}

model users {
  id         Int       @id @default(autoincrement())
  username   String?
  password   String?
  email      String?
  created_at DateTime? @db.Timestamptz(6)
  updated_at DateTime? @db.Timestamptz(6)
}
